# Network Configuration
# Replace YOUR_ALCHEMY_KEY with your actual Alchemy API key
RPC_URL_ETH_MAINNET=https://eth-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
RPC_URL_ETH_SEPOLIA=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
RPC_URL_ARBITRUM_SEPOLIA=https://arb-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# Private Key (for deployment)
# DO NOT commit your private key to version control!
PRIVATE_KEY=your_private_key_here

# Collateral Token Addresses (choose one based on your target network)
# ETH Mainnet
COLLATERAL_ETH_MAINNET=0x833589fCD6eDb6E08f4c7C32D4f71b54bdA02913  # USDC

# ETH Sepolia
COLLATERAL_ETH_SEPOLIA=0x1c7D4B196Cb0C7B01d743Fbc6116a902379C7238  # USDC Sepolia

# Arbitrum Sepolia
COLLATERAL_ARBITRUM_SEPOLIA=0x75faf114eafb1BDbe2F0316DF893fd58CE46AA4d  # USDC Arbitrum

# Deployment Configuration
# Creator override window in seconds (default: 21600 = 6 hours)
CREATOR_OVERRIDE_WINDOW=21600

# Etherscan API Keys (for contract verification)
# Get your API keys from:
# - Etherscan: https://etherscan.io/myapikey
# - Arbiscan: https://arbiscan.io/myapikey
ETHERSCAN_API_KEY=your_etherscan_api_key_here
ARBISCAN_API_KEY=your_arbiscan_api_key_here
